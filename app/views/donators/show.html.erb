<%= form_for @donator do |f| %>

	<%= f.label :Name %> : <%= @donator.name %> <br />
	
	<%= f.label :Alamat %> :<%= @donator.address %><br />
	
	<%= f.label :Telepon %> :<%= @donator.phone %><br />
	<%= f.label :Email %> : <%= @donator.email %><br />
	<%= f.label :'Jenis Kelamin' %> : <%= @donator.gender %><br />
	<%= f.label :Pekerjaan %> : <%= @donator.job %><br />

	<%= link_to "Home", donators_path %>

<% end %>