<h2>List Donatur</h2>

<table>
			<tr>
				<td>Nama</td>
				<td>Email</td>
				<td colspan="3">Aksi</td>
			

			</tr>

<% @donators.each do |donator| %>
	
		
			<tr>
				<td> <%= donator.name %> </td>
				<td> <%= donator.email %> </td>				
				<td> <%= link_to "Edit", edit_donator_path(donator) %> </td>				
				<td> <%= link_to "Delete", donator_path(donator), method: :delete, data: {confirm: "Are you sure ?"} %> </td>				
				
				
			</tr>
	
	<% end %>
		</table> <br />

<%= link_to "Add Donator", new_donator_path %> <br />
